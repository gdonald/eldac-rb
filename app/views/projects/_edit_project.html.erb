<div class="row hidden" id="project">
  <p>&nbsp;</p>
  <div class="panel panel-default">
    <div class="panel-body">
      <%= simple_form_for @project, method: :put do |f| %>
        <%= render partial: 'projects/form', locals: { f: f } %>
        <%= f.button :button, class: 'btn-success' do %>
          <%= sg 'pencil' %>
          Update Project
        <% end %>
      <% end %>
    </div>
  </div>
</div>
